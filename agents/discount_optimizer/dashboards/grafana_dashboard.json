{
  "dashboard": {
    "title": "Shopping Optimizer - Observability Dashboard",
    "tags": ["shopping-optimizer", "agents", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 24,
          "h": 4
        },
        "targets": [
          {
            "expr": "shopping_optimizer_uptime_seconds",
            "legendFormat": "Uptime (seconds)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "none",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "id": 2,
        "title": "Agent Execution Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_agent_duration_seconds_count[5m])",
            "legendFormat": "{{agent}} - executions/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Executions per second",
            "format": "short"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 3,
        "title": "Agent Success Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_agent_success_total[5m]) / (rate(shopping_optimizer_agent_success_total[5m]) + rate(shopping_optimizer_agent_failure_total[5m])) * 100",
            "legendFormat": "{{agent}} - success rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Success Rate (%)",
            "format": "percent",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "min": true,
          "values": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [95],
                "type": "lt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "Agent Success Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Agent Execution Duration (p50, p95, p99)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(shopping_optimizer_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(shopping_optimizer_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(shopping_optimizer_agent_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent}} - p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "label": "Duration (seconds)",
            "format": "s"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 5,
        "title": "API Call Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_api_duration_seconds_count[5m])",
            "legendFormat": "{{api}} - calls/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "API Calls per second",
            "format": "short"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 6,
        "title": "API Success Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_api_success_total[5m]) / (rate(shopping_optimizer_api_success_total[5m]) + rate(shopping_optimizer_api_failure_total[5m])) * 100",
            "legendFormat": "{{api}} - success rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Success Rate (%)",
            "format": "percent",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "min": true,
          "values": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [90],
                "type": "lt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "API Success Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 7,
        "title": "API Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(shopping_optimizer_api_duration_seconds_bucket[5m]))",
            "legendFormat": "{{api}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(shopping_optimizer_api_duration_seconds_bucket[5m]))",
            "legendFormat": "{{api}} - p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(shopping_optimizer_api_duration_seconds_bucket[5m]))",
            "legendFormat": "{{api}} - p99",
            "refId": "C"
          }
        },
        "yaxes": [
          {
            "label": "Latency (seconds)",
            "format": "s"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 0,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["C", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "API Latency Alert (p99 > 5s)",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "shopping_optimizer_cache_hit_rate",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Hit Rate (%)",
            "format": "percent",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "min": true,
          "values": true
        }
      },
      {
        "id": 9,
        "title": "Cache Operations",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 28,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_cache_hits_total[5m])",
            "legendFormat": "Hits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(shopping_optimizer_cache_misses_total[5m])",
            "legendFormat": "Misses/sec",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Operations per second",
            "format": "short"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 10,
        "title": "Error Rate by Type",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 36,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(shopping_optimizer_agent_failure_total[5m])",
            "legendFormat": "Agent: {{agent}}",
            "refId": "A"
          },
          {
            "expr": "rate(shopping_optimizer_api_failure_total[5m])",
            "legendFormat": "API: {{api}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Errors per second",
            "format": "short"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "annotations": {
      "list": [
        {
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    }
  }
}
